{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "UrlEncoder",
  "description": "URLEncoder / Decoder",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "URLEncoder /Decoder",
    "default_popup": "popup.html"
  },
  "icons" : {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["oncall.js"]
    }
  ],
  "permissions": [
    "tabs",
    "background",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}